{
  "name": "scamplers-ui",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "typedefs": "bunx json-schema-to-typescript --input ../json-schema/ --output src/lib/scamplers-models/",
    "dev": "bun run typedefs && vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "devDependencies": {
    "@sveltejs/adapter-node": "^5.4.0",
    "@sveltejs/kit": "^2.48.4",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@types/bun": "latest",
    "svelte": "^5.43.6",
    "svelte-check": "^4.3.4",
    "typescript": "^5",
    "vite": "^7.2.2"
  },
  "dependencies": {
    "better-auth": "1.4.0-beta.19",
    "qs": "^6.14.0"
  }
}
